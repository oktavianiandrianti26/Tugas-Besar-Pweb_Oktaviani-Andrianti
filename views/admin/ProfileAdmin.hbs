        <label for="address" class="self-start mt-7 ml-3.5 max-md:ml-2.5">Alamat</label>
        <input
          type="text"
          id="address"
          class="justify-center items-start px-6 py-5 mt-2 whitespace-nowrap bg-white rounded-3xl text-zinc-600 text-opacity-50 max-md:px-5 max-md:max-w-full"
          placeholder="Alamat"
          aria-label="Enter your address"
        />
        <label for="role" class="self-start mt-8 ml-3.5 max-md:ml-2.5">Role</label>
        <input type="text" id="role" class="justify-center items-start px-6 py-5 mt-1.5 bg-white rounded-3xl text-zinc-600 text-opacity-50 max-md:px-5 max-md:max-w-full" placeholder="Masukkan nama" aria-label="Enter your role" />
        <div class="flex gap-5 justify-between self-end mt-10 max-md:flex-wrap">
          <button class="justify-center px-7 py-3 rounded-3xl bg-slate-700 max-md:px-5"><a href="password">Edit Profile</a></button>
          <button class="justify-center px-14 py-2.5 rounded-3xl bg-slate-700 max-md:px-5">Simpan</button>
        </div>
