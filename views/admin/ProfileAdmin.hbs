    <main class="flex flex-col mt-10 mb-10 ml-5 w-[72%] max-md:ml-0 max-md:w-full">
      <section aria-labelledby="profile-section" class="flex flex-col self-stretch px-14 py-8 my-auto w-full text-xl font-semibold text-white rounded-3xl bg-slate-500 max-md:px-5 max-md:mt-10 max-md:max-w-full">
